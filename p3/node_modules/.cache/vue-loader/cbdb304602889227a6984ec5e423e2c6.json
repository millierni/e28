key=\"item\" v-for=\"item in miners\">\n              <th scope=\"row\" class=\"brand\">{{ item.brand }}</th>\n              <td>{{ item.model }}</td>\n              <td>{{ item.hashingAlgorithm }}</td>\n              <td>{{ item.hashrate }}</td>\n              <td>{{ item.power }}</td>\n            </tr>\n\n          </tbody>\n\n        </table>\n      </div>\n\n    </div>\n</template>\n\n<script>\n\nexport default {\n  props: {\n    miners: {\n      type: Array,\n      default: null,\n    }\n  },\n\n  setup() {\n    function countMiners() {\n      this.$store.commit(\"setMinersCount\", this.miners.length);\n    }\n\n    return {\n      countMiners,\n    };\n  },\n\n  computed: {\n    minersCount() {\n      this.countMiners();\n      return this.$store.state.minersCount;\n    }\n  },\n}\n</script>\n\n<style>\n#count {\n  font-size: 25px;\n}\n</style>"]}]}